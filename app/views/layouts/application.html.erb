<!DOCTYPE html>
<html>
<head>
  <title>SideTable</title>
  <%= stylesheet_link_tag    'application', media: 'all' %>
  <%= javascript_include_tag 'application' %>
  <%= filepicker_js_include_tag %>
  <%= csrf_meta_tags %>
</head>
<body>
  <header class="global-header"> </header>
  <% flash[:errors].each do |message| %>
    <h3> <%= message %> </h3>
  <% end if flash[:errors] %>

<%= yield %>

</body>
</html>
